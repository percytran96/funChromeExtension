{
	"manifest_version": 2,

	"name": "CSKitten",
	"description": "A simple page-scraping extension for Chrome",
	"version": "1.0",
	"author": "@thomasforth",



	"permissions": [
        "tabs",
        "http://*/",
        "https://*/"
    ],
	"browser_action": {
		"default_icon": "logo.png",
		"default_popup": "popup.html"
	}
}
